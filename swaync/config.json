{
  "positionX": "right",
  "positionY": "top",
  "layer": "overlay",
  "control-center-layer": "top",
  "layer-shell": true,
  "cssPriority": "application",
  "notification-window-width": 500,
  "notification-body-image-height": 100,
  "notification-body-image-width": 200,
  "timeout": 10,
  "timeout-low": 5,
  "timeout-critical": 0,
  "notification-padding": 8,
  "notification-spacing": 8,
  "notification-icon-size": 64,
  "notification-rounding": 12,
  "control-center-width": 500,
  "control-center-height": 600,
  "control-center-spacing": 8,
  "control-center-padding": 8,
  "control-center-rounding": 12,
  "script-fail-notify": true,
  "scripts": {},
  "widgets": [
    "title",
    "dnd",
    "notifications"
  ],
  "widget-config": {
    "title": {
      "text": "Notifications",
      "clear-all-button": true,
      "button-text": "Clear All"
    },
    "dnd": {
      "text": "Do Not Disturb"
    },
    "notifications": {
      "label": "Notifications",
      "group-by": "app_name"
    }
  }
}